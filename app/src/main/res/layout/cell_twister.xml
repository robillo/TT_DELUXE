<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidUnknownAttribute -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/numberHolderLayout">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/number_bg_circle"
            android:padding="@dimen/padding_normal_smaller"
            tools:ignore="ContentDescription" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/black"
            android:textSize="@dimen/font_normal"
            android:gravity="center"
            fontPath="fonts/WS-SemiBold.ttf"
            tools:textStyle="bold"
            tools:text="65"
            tools:ignore="MissingPrefix"
            android:id="@+id/twisterNumberTv"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/numberHolderLayout"
        app:layout_constraintBottom_toBottomOf="@id/numberHolderLayout"
        app:layout_constraintStart_toEndOf="@id/numberHolderLayout"
        app:layout_constraintEnd_toStartOf="@id/lockHolderLayout"
        android:textSize="@dimen/font_large"
        android:textColor="@color/black"
        fontPath="fonts/WS-SemiBold.ttf"
        android:paddingStart="@dimen/padding_normal"
        tools:text="Inside Out"
        tools:textStyle="bold"
        tools:ignore="MissingPrefix,RtlSymmetry" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/numberHolderLayout"
        app:layout_constraintBottom_toBottomOf="@id/numberHolderLayout"
        android:id="@+id/lockHolderLayout">

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>